{
  "name": "build-python",
  "version": "0.13.1",
  "description": "Atom Build provider for python/python3/py, runs Python",
  "repository": "https://github.com/idleberg/atom-build-python",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "npm run start",
    "lint": "eslint ./src --ignore-path .gitignore",
    "start": "webpack --mode none --watch",
    "test": "npm run lint"
  },
  "keywords": [
    "buildprovider",
    "compile",
    "python",
    "python3",
    "linter",
    "lint"
  ],
  "main": "lib/provider.js",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "builder": {
      "description": "Runs Python",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "package-deps": [
    [
      {
        "name": "buildium"
      },
      {
        "name": "build"
      }
    ]
  ],
  "dependencies": {
    "@atxm/developer-console": "^0.5.0",
    "atom-satisfy-dependencies": "^0.3.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/eslint-parser": "^7.13.10",
    "babel-loader": "^8.0.6",
    "eslint": "^7.22.0",
    "husky": ">= 4 <5",
    "lint-staged": ">=10",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
